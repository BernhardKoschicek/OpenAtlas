{
  "components": {
    "parameters": {
      "columnParam": {
        "description": "The result will be sorted by the given column",
        "in": "query",
        "name": "column",
        "schema": {
          "enum": [
            "id",
            "classCode",
            "name",
            "description",
            "created",
            "modified",
            "systemClass",
            "beginFrom",
            "beginTo",
            "endFrom",
            "endTo"
          ],
          "type": "string"
        }
      },
      "countParam": {
        "description": "Returns a number which represents the total count of the result",
        "in": "query",
        "name": "count",
        "schema": {
          "type": "boolean"
        }
      },
      "downloadParam": {
        "description": "Triggers the file download of the given request",
        "in": "query",
        "name": "download",
        "schema": {
          "type": "boolean"
        }
      },
      "exportParam": {
        "description": "Select to which format the output is downloaded",
        "in": "query",
        "name": "export",
        "schema": {
          "enum": [
            "csv"
          ],
          "type": "string"
        }
      },
      "filterParam": {
        "description": "Specify request with custom SQL filter method. ",
        "deprecated": true,
        "in": "query",
        "name": "filter",
        "schema": {
          "type": "string"
        }
      },
      "searchParam": {
        "description": "Search request with AND/OR logic. Confer https://redmine.craws.net/projects/uni/wiki/API_03#Search",
        "in": "query",
        "name": "search",
        "schema": {
          "type": "string"
        }
      },
      "firstParam": {
        "description": "List of results start with given ID",
        "in": "query",
        "name": "first",
        "schema": {
          "type": "number"
        }
      },
      "pageParam": {
        "description": "Jump to given page",
        "in": "query",
        "name": "page",
        "schema": {
          "type": "number"
        }
      },
      "formatParam": {
        "description": "Select to which output format is prefered",
        "in": "query",
        "name": "format",
        "schema": {
          "enum": [
            "linked_places",
            "geojson"
          ],
          "type": "string"
        }
      },
      "geometryParam": {
        "description": "Filter, which geometric entities will be retrieved through /geometric_entities",
        "in": "query",
        "name": "geometry",
        "schema": {
          "enum": [
            "gisAll",
            "gisPointAll",
            "gisPointSupers",
            "gisPointSubs",
            "gisPointSibling",
            "gisLineAll",
            "gisPolygonAll"
          ],
          "type": "string"
        }
      },
      "geometryParam": {
        "description": "Select geometry types.",
        "in": "query",
        "name": "geometry",
        "schema": {
          "enum": [
            "gisAll",
            "gisPointAll",
            "gisPointSupers",
            "gisPointSubs",
            "gisPointSibling",
            "gisLineAll",
            "gisPolygonAll"
          ],
          "type": "string"
        }
      },
      "langParam": {
        "description": "Select output language",
        "in": "query",
        "name": "language",
        "schema": {
          "enum": [
            "en",
            "de"
          ],
          "type": "string"
        }
      },
      "lastParam": {
        "description": "List of results start with entity after given ID",
        "in": "query",
        "name": "last",
        "schema": {
          "type": "number"
        }
      },
      "limitParam": {
        "description": "Number of entities returned per page",
        "in": "query",
        "name": "limit",
        "schema": {
          "type": "number"
        }
      },
      "showParam": {
        "description": "Select which key should be shown. If 'not' is used, no other keys will be shown",
        "in": "query",
        "name": "show",
        "schema": {
          "enum": [
            "when",
            "types",
            "relations",
            "names",
            "links",
            "geometry",
            "depictions",
            "not"
          ],
          "type": "string"
        }
      },
      "sortParam": {
        "description": "Result will be sorted asc/desc (default column is name)",
        "in": "query",
        "name": "sort",
        "schema": {
          "enum": [
            "asc",
            "desc"
          ],
          "type": "string"
        }
      },
      "typeParam": {
        "description": "The output will be filtered by the type_id. Only entities with this type ids will be display. The relation is in logical OR.",
        "in": "query",
        "name": "type_id",
        "schema": {
          "type": "number"
        }
      }
    },
