{
  "components": {
    "parameters": {
      "columnParam": {
        "description": "The result will be sorted by the given column",
        "in": "query",
        "name": "column",
        "schema": {
          "enum": [
            "id",
            "classCode",
            "name",
            "description",
            "created",
            "modified",
            "systemClass",
            "beginFrom",
            "beginTo",
            "endFrom",
            "endTo"
          ],
          "type": "string"
        }
      },
      "countParam": {
        "description": "Returns a integer which represents the total count of the result",
        "in": "query",
        "name": "count",
        "schema": {
          "type": "boolean"
        }
      },
      "downloadParam": {
        "description": "Triggers the file download of the given request",
        "in": "query",
        "name": "download",
        "schema": {
          "type": "boolean"
        }
      },
      "filterParam": {
        "description": "Specify request with custom SQL filter method. ",
        "deprecated": true,
        "in": "query",
        "name": "filter",
        "schema": {
          "type": "string"
        }
      },
      "searchParam": {
        "description": "Search request with AND/OR logic. Confer https://redmine.craws.net/projects/uni/wiki/API_03#Search",
        "in": "query",
        "name": "search",
        "schema": {
          "type": "string"
        }
      },
      "firstParam": {
        "description": "List of results start with given ID",
        "in": "query",
        "name": "first",
        "schema": {
          "type": "integer"
        }
      },
      "pageParam": {
        "description": "Jump to given page",
        "in": "query",
        "name": "page",
        "schema": {
          "type": "integer"
        }
      },
      "formatParam": {
        "description": "Select to which output format is prefered",
        "in": "query",
        "name": "format",
        "schema": {
          "enum": [
            "linked_places",
            "geojson"
          ],
          "type": "string"
        }
      },
      "geometryParam": {
        "description": "Filter, which geometric entities will be retrieved through /geometric_entities",
        "in": "query",
        "name": "geometry",
        "schema": {
          "enum": [
            "gisAll",
            "gisPointAll",
            "gisPointSupers",
            "gisPointSubs",
            "gisPointSibling",
            "gisLineAll",
            "gisPolygonAll"
          ],
          "type": "string"
        }
      },
      "langParam": {
        "description": "Select output language",
        "in": "query",
        "name": "lang",
        "schema": {
          "enum": [
            "en",
            "de"
          ],
          "type": "string"
        }
      },
      "lastParam": {
        "description": "List of results start with entity after given ID",
        "in": "query",
        "name": "last",
        "schema": {
          "type": "integer"
        }
      },
      "limitParam": {
        "description": "Integer of entities returned per page. 0 means no limit is set. Default is 20.",
        "in": "query",
        "name": "limit",
        "schema": {
          "type": "integer"
        }
      },
      "showParam": {
        "description": "Select which key should be shown. If 'not' is used, no other keys will be shown",
        "in": "query",
        "name": "show",
        "schema": {
          "enum": [
            "when",
            "types",
            "relations",
            "names",
            "links",
            "geometry",
            "depictions",
            "not"
          ],
          "type": "string"
        }
      },
      "relationTypeParam": {
        "description": "Select which relations are shown",
        "in": "query",
        "name": "relation_type",
        "schema": {
          "example": "P2",
          "type": "string"
        }
      },
      "sortParam": {
        "description": "Result will be sorted asc/desc (default column is name)",
        "in": "query",
        "name": "sort",
        "schema": {
          "enum": [
            "asc",
            "desc"
          ],
          "type": "string"
        }
      },
      "exportParam": {
        "description": "Result will be downloaded in the given format.",
        "in": "query",
        "name": "export",
        "schema": {
          "enum": [
            "csv",
            "csvNetwork"
          ],
          "type": "string"
        }
      },
      "typeParam": {
        "description": "The output will be filtered by the type_id. Only entities with this type ids will be display. The relation is in logical OR.",
        "in": "query",
        "name": "type_id",
        "schema": {
          "type": "integer"
        }
      }
    },
